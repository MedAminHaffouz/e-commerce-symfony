
{% extends 'base.html.twig' %}

{% block title %}Add New User{% endblock %}

{% block body %}
    <h1>Add a New User</h1>

    {{ form_start(form) }}
    {{ form_row(form.firstName) }}
    {{ form_row(form.lastName) }}
    {{ form_row(form.email) }}
    {{ form_end(form) }}
    <button class="btn btn-success">Create User</button>

{% endblock %}
